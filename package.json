{
  "name": "qubic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build-css": "stylus style --out public/stylesheets",
    "watch-css": "nodemon -e styl -x \"npm run build-css\"",
    "build-js": "browserify client/main.js -o public/javascript/main.js",
    "watch-js": "watchify client/main.js -o public/javascript/main.js -v",
    "watch-assets": "npm run watch-css & npm run watch-js",
    "watch-server": "nodemon --ignore style/ --ignore public/ --ignore client/ --ignore test/ ./bin/www --ignore node_modules/",
    "watch": "npm run watch-assets & npm run watch-server",
    "watch-win": "start npm run watch-css & start npm run watch-js & start npm run watch-server",
    "build": "npm run build-js & npm run build-css",
    "test": "mocha",
    "dev": "DEBUG=qube-web:* NODE_ENV=development npm run watch",
    "dev-win": "set DEBUG=qube-web:*&& set NODE_ENV=development&& npm run watch-win",
    "install-windows-service": "winser -i --startcmd=\"node ./bin/www\"",
    "uninstall-windows-service": "winser -r"
  },
  "author": {
    "name": "Benjamin Norrington",
    "email": "ben@norrington.net"
  },
  "license": "Proprietary",
  "dependencies": {
    "axis": "^0.3.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "browserify": "^14.4.0",
    "browserify-shim": "~3.8.12",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.4",
    "cookie-session": "^1.3.2",
    "copyfiles": "^0.2.1",
    "dummy-sharejs-connection": "git://github.com/mavxg/dummy-sharejs-connection",
    "encrypted-context": "git://github.com/:mavxg/encrypted-context",
    "express": "^4.16.3",
    "express-ntlm": "^2.1.4",
    "express-session": "^1.15.6",
    "font-awesome": "^4.4.0",
    "friar": "git://github.com/mavxg/friar",
    "geopattern": "git://github.com/mavxg/geopattern",
    "hat": "0.0.3",
    "kbpgp": "^2.0.35",
    "method-override": "^2.3.10",
    "moment": "^2.19.3",
    "mongodb": "^2.2.34",
    "mongoose": "^4.13.14",
    "morgan": "^1.9.0",
    "openpgp": "~1.5.7",
    "ot-sexpr": "git://github.com/mavxg/ot-sexpr",
    "passport": "^0.2.1",
    "passport-custom": "^1.0.1",
    "passport-facebook": "^2.0.0",
    "passport-github": "^0.1.5",
    "passport-google-oauth": "^0.2.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "pug": "^2.0.3",
    "qube": "git://github.com/mavxg/qube",
    "qube-excel": "git://github.com/mavxg/qube-excel",
    "serve-favicon": "^2.5.0",
    "sharedb": "^1.0.0-beta.8",
    "sharedb-mongo": "^1.0.0-beta.4",
    "slate0": "git://github.com/mavxg/slate0",
    "slatejs": "git://github.com/mavxg/slatejs",
    "slatejs-encryption": "git://github.com/mavxg/slatejs-encryption",
    "slatejs-qube": "git://github.com/mavxg/slatejs-qube",
    "stylus": "^0.54.5",
    "uglify-js": "^3.3.28",
    "undomanager": "git://github.com/mavxg/undomanager",
    "websocket-json-stream": "^0.0.1",
    "winser": "1.0.0",
    "ws": "^1.1.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "parallelshell": "^2.0.0",
    "watchify": "^3.11.0"
  }
}
